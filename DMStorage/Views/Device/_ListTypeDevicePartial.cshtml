@model DMStorage.Core.ViewModels.ListsViewModel

<h6 style="color: #39cf6a">Seznam Typu zarizeni</h6>
<div class="my-border">

    <ul class="col-12 scrollable-list">
        @foreach (var typ in Model.TypeDevices)
        {
            <li>
                <a href="@Url.Action("Details", "Home", new { type = "deviceType", id = typ.Id })">
                    <div class="row p-0 m-0">
                        <p>@typ.Name</p>
                    </div>
                </a>
            </li>
        }
    </ul>
</div>