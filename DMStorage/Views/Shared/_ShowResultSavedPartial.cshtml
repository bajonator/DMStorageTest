@model IEnumerable<DMStorage.Core.Models.Domains.Machine>

<div class="container">
    <h3 class="text-center text-info">Neulozene polozky</h3>
    @if (Model.Any())
    {
        @foreach (var machine in Model)
        {
            <div class="row justify-content-between border-bottom border-secondary">
                <div class="col">Nazev stroje:</div>
                <div class="col">@machine.Name</div>
            </div>
        }
    }
</div>
